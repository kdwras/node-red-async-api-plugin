{
  "name": "openapi-red",
  "version": "2.2.2",
  "description": "OpenAPI (Swagger) client for node-red ",
  "author": "Sebastian Meier",
  "contributors": [
    "Sebastian Meier",
    "Christian Geiger"
  ],
  "main": "src/openApi-red.js",
  "scripts": {
    "setup": "npm install && mkdir -p /home/gitpod/.node-red/ && cd /home/gitpod/.node-red/ && npm link /workspace/openApi-red --save && cd /workspace/openApi-red",
    "dev": "node-red examples/petstore.json"
  },
  "keywords": [
    "node-red",
    "swagger",
    "swagger client",
    "openApi",
    "openApi client",
    "open api",
    "open api client",
    "open",
    "api"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/2WeltenChris/openapi-red.git"
  },
  "license": "MIT",
  "node-red": {
    "version": ">=3.0.0",
    "nodes": {
      "openApi-red": "src/openApi-red.js",
      "openApi-red-url": "src/urlConfig.js"
    }
  },
  "engines": {
    "node": ">=18.0"
  },
  "dependencies": {
    "swagger-client": "^3.32.2",
    "tinypool": "^1.0.2",
    "yaml": "^2.6.1"
  },
  "devDependencies": {
    "standard": "^17.1.2",
    "svelte-integration-red": "^2.4.3"
  }
}
